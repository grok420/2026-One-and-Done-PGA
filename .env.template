# PGA One and Done Optimizer Configuration
# Copy this file to .env and fill in your values

# BuzzFantasyGolf.com credentials
PGA_OAD_EMAIL=your_email@example.com
PGA_OAD_PASSWORD=your_password
PGA_OAD_USERNAME=your_username

# Data Golf API Key (get from datagolf.com)
DATAGOLF_API_KEY=your_api_key_here

# The Odds API Key (get from the-odds-api.com) - Optional
ODDS_API_KEY=your_odds_api_key_here

# Strategy settings
PGA_OAD_RISK_LEVEL=5
